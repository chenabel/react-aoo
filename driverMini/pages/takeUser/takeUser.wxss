.pageCentent{
  background: rgb(242,242,242);
  position: fixed;
  bottom: 100rpx;
  top: 0rpx;
  left: 0rpx;
  right: 0rpx;
}


.zhuTi{
  width: 700rpx;
  position: fixed;
  top: 0rpx;
  bottom: 100rpx;
  left: 25rpx;
}

.pageBody{
  width: 100%;
  height: 94%;
  background:white;
  margin-top: 3%;
  box-shadow: 0 10px 20px 0 rgb(65 65 65 / 10%);
  border:1px solid #fff;
  border-radius: 40rpx;
}

.bodyHead{
  width: 90%;
  height: 10%;
  margin: 0 auto;
  font-size: 45rpx;
  border-bottom: 2rpx solid #f7f7f7;
  display: flex;
  align-items: center;
}

.body{
  width: 85%;
  height: 80%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}

.bodyFeet{
  width: 100%;
  height: 10%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 100rpx;
  margin: 0 auto;
}
.body view{
  font-size: 40rpx;
  color: #5a5959;
}
.iconfont{
  font-size: 45rpx;
}
.bodyFeetLeft{
  width: 50%;
  height: 100%;
  text-align: center;
  border-radius: 40rpx 40rpx 40rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.bodyFeetRight{
  width: 50%;
  height: 100%;
  text-align: center;
  background: orange;
  border-radius: 40rpx 40rpx 39rpx 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.orderItem{
  margin-left: 20rpx;
}
.wholeCourse{
  color: #eeba6c;
}
.allPrice{
  color: #5a5959;
}