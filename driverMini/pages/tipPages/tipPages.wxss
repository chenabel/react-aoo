.zhuTi{
  width: 700rpx;
  position: fixed;
  top: 0rpx;
  bottom: 0rpx;
  left: 25rpx;
  text-align: center;
}

.pageHead{
  width: 750rpx;
}

.scroll{
  width: 100%;
  margin:0 auto;
  overflow: scroll;
  height: 100%;
}

.dataFrom{
  width: 650rpx;
  height: 100rpx;
  line-height: 60rpx;
  display: flex;
  flex-direction: row;
  margin: 0 auto;
  align-items: center;
}
.fromTitle{
  line-height: 100rpx;
}
.view-contain-ti {
  display: flex;
  height: 40px;
  margin: 20px;
  border: 5rpx solid #faca82;
  border-radius: 10rpx;
}

.text-ti {
  position: absolute;
  font-size: 12px;
  background: white;
  margin: -10px 0 0 10px;
  padding: 0 5px;
  color: rgb(144, 147, 167);
}

.input1 {
  margin:  auto 10px;
}
.fileBox{
  width: 700rpx;
  display: flex;
  flex-direction: row;
  text-align: center;
}
.fileImg{
  width: 200rpx;
  margin:0 auto ;
}
.imgAndErr{
  width: 200rpx;
  height: 200rpx;
}
.photos{
  width: 200rpx;
  height: 200rpx;
  margin:0 auto;
}
.title{
  color: rgb(148, 150, 167);
}
.btn1 {
  width: 80%;
  margin-top: 20rpx;
  background-color: rgb(148, 150, 167);
  color: white;
  border-radius: 98rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.btnImg {
  margin-right: 8rpx;
  width: 46rpx;
  height: 46rpx;
}

.btn1::after {
  border-radius: 98rpx;
  border: 0; 
}
